          # Set src to the localFile path
          src = localFile;

#          nativeBuildInputs = [
#            autoPatchelfHook
#          ];

          buildInputs = [
#            zlib
#            bzip2
#            curlMinimal
#            freeimage
#            freetype
#            glew
#            sfml
#            ftgl
#            fmt
#            libGLU
#            libglvnd
#            mpg123
#            gtk3
#            libcanberra-gtk3
