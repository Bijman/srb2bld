    cp -f "\$BUILD/build/AppDir/usr/share/applications/srb2-project-astral-software.desktop" "\$BUILD/build/AppDir/usr/share/applications/srb2-project-astral-opengl.desktop"
    gawk -i inplace '{gsub("Name=Sonic Robo Blast 2 Project Astral \\\(Software\\\)","Name=Sonic Robo Blast 2 Project Astral (OpenGL)") || gsub("Exec=$SRB2BLDPREFIX/bin/srb2pa -software","Exec=$SRB2BLDPREFIX/bin/srb2pa -opengl"); print}' "\$BUILD/build/AppDir/usr/share/applications/srb2-project-astral-opengl.desktop"

    elif [ "\$BUILD" = "\$BUILDPATH/srb2-vr" ]; then

        cat > "\$BUILD/build/AppDir/usr/share/applications/srb2-vr-software.desktop" << DESKTOP
[Desktop Entry]
Version=\$COMREV
Type=Application
Name=Sonic Robo Blast 2 VR (Software)
Comment=Modified open source 3D Sonic VR platformer with improved framerate and additional features
