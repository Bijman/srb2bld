FROM docker.io/debian:"$DEBVER"

ENV container $(printf "%s\n" "$OSVIRT" | gawk '{print $1}')

ENV DEBIAN_FRONTEND noninteractive

ENV GNUTLS_CPUID_OVERRIDE 0x1

