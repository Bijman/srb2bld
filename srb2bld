    elif [ "\$(dirname \$BUILD)" = "\$BUILDPATH/srb2-custom" ] || [ "\$(dirname \$BUILDMAIN)" = "\$BUILDPATH/srb2-custom" ]; then

        SRB2CNAME="\$SRB2CCONFDIR"c"\$CUSTOMDIR"

        cat > "\$BUILD/build/AppDir/usr/share/applications/\$SRB2CNAME-software.desktop" << DESKTOP
