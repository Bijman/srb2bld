    gawk -i inplace '{gsub("Name=Dr. Robotnik'\''s Ring Racers HEP3 \\\(Software\\\)","Name=Dr. Robotnik'\''s Ring Racers HEP3 (OpenGL)") || gsub("Exec=$SRB2BLDPREFIX/bin/ringracershep3 -software","Exec=$SRB2BLDPREFIX/bin/ringracershep3 -opengl"); print}' "\$BUILD/build/AppDir/usr/share/applications/ring-racers-hep3-opengl.desktop"

    elif [ "\$(basename \$BUILD)" = "ring-racers-freaky-build" ]; then

