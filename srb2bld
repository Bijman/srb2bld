        $PRINTF "\n$PROMPT==> $MESSAGE%s\n\e[0m" "Installing $BUILDNAME's srb2kartneptune binary, assets, icons and desktop file to the system. Please wait..."

        if [ "$LNXOSNAME" != "NixOS" ]; then

